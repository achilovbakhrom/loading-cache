{
  "name": "loading-cache",
  "version": "0.4.0",
  "description": "Loading Cache Implementation for JS",
  "main": "index.js",
  "scripts": {
    "all": "run-s lint cover build",
    "test": "jest",
    "cover": "jest --coverage --no-cache",
    "codecov": "codecov -f ./coverage/lcov.info",
    "lint": "tslint \"src/**/**.ts\"",
    "format": "prettier --write \"src/**/**.ts\"",
    "build": "run-s build:*",
    "build:es": "rimraf ./es && rollup -c ./rollup.config.js",
    "build:lib": "rimraf ./lib && tsc --project tsconfig.lib.json",
    "build:node": "rimraf ./node && tsc --project tsconfig.node.json",
    "report": "opener ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achilovbakhrom/loading-cache.git"
  },
  "keywords": [
    "rxjs",
    "cache",
    "promise",
    "memoization"
  ],
  "author": "mailbak36",
  "license": "MIT",
  "homepage": "https://github.com/achilovbakhrom/loading-cache#readme",
  "dependencies": {
    "tslib": "^1.7.0"    
  },
  "optionalDependencies": {
    "rxjs": "^5.5.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {    
    "@types/jest": "^21.1.4",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.26",
    "codecov": "^2.3.0",
    "jest": "^21.0.1",
    "npm-run-all": "^4.1.1",
    "opener": "^1.4.3",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.7.0",
    "ts-jest": "^21.0.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}
